{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to packager",
      "request": "attach",
      "type": "reactnative",
      "cwd": "${workspaceFolder}",
      "port": 8083,
    },

    {
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "request": "attach",
      "type": "node",
      "localRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "attachExistingChildren": false,
      "port": 8083,
      "cwd": "${workspaceFolder}",
      "sourceMapRenames": true,
      "sourceMapPathOverrides": {
        "meteor://ðŸ’»app/*": "${workspaceFolder}/buildHooks/index.js",
        "webpack:///./~/*": "${workspaceFolder}/node_modules/*",
        "webpack://?:*/*": "${workspaceFolder}/*"
      }
    },
    {
      "command": "rnv start -p android",
      "name": "Run rnv start",
      "request": "launch",
      "type": "node-terminal",
      "localRoot": "${workspaceFolder}"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://127.0.0.1:8080",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Attach to A packager",
      "program": "${workspaceRoot}/.vscode/launchReactNative.js",
      "type": "reactnative",
      "request": "attach",
      "cwd": "${workspaceFolder}",
      "sourceMaps": false
    },
    {
      "type": "chrome",
      "name": "http://localhost:8083/debugger-ui",
      "request": "launch",
      "url": "http://localhost:8083/debugger-ui"
    }
  ]
}
